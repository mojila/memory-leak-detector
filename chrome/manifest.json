{
    "name": "Meleak",
    "description": "Extends the developer tools",
    "version": "1.0",
    "author": "Mojila",
    "manifest_version": 2,
    "devtools_page": "devtools.html",
    "browser_action": {
        "default_popup": "popup/index.html",
        "default_title": "Memory Leak Detector"
    },
    "permissions": [
        "activeTab"
    ]
}